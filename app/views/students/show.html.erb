<h1><%= @student.branch%>'s <%= @student.first_name%> <%= @student.last_name%></h1>
<h3>Email: <%= @student.email %> </h3>
<h2>Cohort</h2>
<div id="student_cohort">
    <table>
        <thead>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </thead>
        <tbody>
            <tr>
                <% @cohort.each do |cohort| %>
                <td><%= cohort.first_name%></td>
                <td><%= cohort.last_name%></td>
                <td><%= cohort.email%></td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>
<br><a href="/dojos">Home</a> | <a href="/dojos/<%=@student.dojo_id%>/students/<%=@student.id%>/edit">Edit</a>
